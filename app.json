{
  "expo": {
    "name": "tenpo-eats",
    "slug": "tenpo-front-mobile-jsx-challenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Maquetación de las principales vistas de una app de venta electrónica de comidas usando react-native ft expo",
    "githubUrl": "https://github.com/cnerylozada/tenpo.front.mobile.jsx.challenge"
  }
}
